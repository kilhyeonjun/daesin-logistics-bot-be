{
  "name": "logistics-bot",
  "version": "1.0.0",
  "type": "module",
  "description": "대신물류 배차현황 크롤링 및 카카오톡 챗봇",
  "main": "dist/server.js",
  "scripts": {
    "build": "prisma generate && tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "dev:old": "tsx watch src/app.ts",
    "sync": "tsx -e \"import { container } from './src/config/container.js'; import { configureContainer } from './src/config/container.js'; import { SyncRoutesUseCase } from './src/application/use-cases/SyncRoutesUseCase.js'; configureContainer(); const sync = container.resolve(SyncRoutesUseCase); await sync.execute();\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "npm run build && tsx scripts/integration-test.ts",
    "test:crawl": "tsx test-crawl.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "iconv-lite": "^0.7.1",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^4.2.1",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/iconv-lite": "^0.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/node-cron": "^3.0.11",
    "prisma": "^6.19.1",
    "supertest": "^7.1.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
